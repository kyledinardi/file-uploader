<%- include('header', {title}); %>
<main>
  <h1>Are you sure you want to delete <%= name %>?</h1>
  <div class="buttons">
    <form method="post">
      <button>Delete</button>
    </form>
    <a href="<%= folder.isIndex ? '/' :  `/folders/${folder.id}` %>">
      <button>Cancel</button>
    </a>
  </div>
</main>
<%- include('footer'); %>
